{
  "presets": [
    ["next/babel",
    {
      "preset-react": {
        "runtime": "automatic",
        "importSource": "@emotion/react"
      }
    }]
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true,
      }
    ],
    "@emotion/babel-plugin"
  ]
}